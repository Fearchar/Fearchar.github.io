{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 7,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react-hooks"],
  "rules": {
    "brace-style": "error",
    "camelcase": ["error", { "properties": "never" }],
    "func-call-spacing": ["error", "never"],
    "eqeqeq": "warn",
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "key-spacing": ["error", { "beforeColon": false }],
    "no-console": "error",
    "no-fallthrough": "warn",
    "prefer-const": "error",
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "react/prop-types": [0],
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "curly": ["error", "all"]
  }
}
